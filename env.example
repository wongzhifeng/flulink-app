# FluLink项目环境变量配置

# 数据库配置
TURSO_DATABASE_URL=libsql://your-database-url.turso.io
TURSO_AUTH_TOKEN=your-auth-token
TURSO_SYNC_URL=https://your-sync-url.turso.io

# 应用配置
NODE_ENV=production
PORT=3000

# 安全配置
JWT_SECRET=your-jwt-secret
ENCRYPTION_KEY=your-encryption-key

# 地理服务配置
AMAP_API_KEY=your-amap-api-key
AMAP_SECRET_KEY=your-amap-secret-key

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=json

# 性能配置
MAX_CONNECTIONS=1000
QUERY_TIMEOUT=5000
CACHE_TTL=300

# 边缘计算配置
EDGE_REGIONS=us-east,us-west,eu-west,asia-east
SYNC_INTERVAL=300
MAX_SYNC_DELAY=5000
